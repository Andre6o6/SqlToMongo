<Window x:Class="SqlToMongo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SqlToMongo"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="buttonInspectMongo" Content="Mongo" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="buttonInspectMongo_Click"/>
        <Button x:Name="buttonInspectPostgres" Content="SQL" HorizontalAlignment="Right" Margin="0,10,10,0" VerticalAlignment="Top" Width="75" Grid.Column="1" Click="buttonInspectPostgres_Click"/>
        <ListBox x:Name="listBox" Height="150" Margin="10,35,104,0" VerticalAlignment="Top"/>
        <ListBox x:Name="listBox1" Height="150" Margin="93,35,10,0" VerticalAlignment="Top" Grid.Column="1"/>
        <Button x:Name="button" Content="=&gt;" Margin="0,102,0,0" VerticalAlignment="Top" Grid.Column="1" HorizontalAlignment="Left" Width="25"/>
        <Button x:Name="button1" Content="&lt;=" Margin="0,82,0,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="25"/>

    </Grid>
</Window>
